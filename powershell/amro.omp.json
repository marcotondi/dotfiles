{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "\ueb99 {{ .UserName }} |",
          "type": "session"
        },
        {
          "foreground": "#0CA0D8",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#FFE873",
          "foreground_templates": [
            "{{ if .Venv }}#00D787{{ end }}"
          ],
          "properties": {
            "fetch_virtual_env": true,
            "home_enabled": false
          },
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue235{{ if .Venv }} [\uf10c {{ .Venv }}]{{ end }} {{ .Full }} {{ end }}",
          "type": "python"
        },
        {
          "foreground": "#E76F00",
          "properties": {},
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue738 {{ .Full }} {{ end }}",
          "type": "java"
        },
        {
          "foreground": "#3C873A",
          "properties": {
            "fetch_package_manager": true
          },
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} {{ end }}",
          "type": "node"
        },
        {
          "foreground": "#00ADD8",
          "properties": {},
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue626 {{ .Full }} {{ end }}",
          "type": "go"
        },
        {
          "foreground": "#CE422B",
          "properties": {},
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue7a8 {{ .Full }} {{ end }}",
          "type": "rust"
        },
        {
          "foreground": "#512BD4",
          "properties": {},
          "style": "plain",
          "template": "{{ if .Error }}{{ else }}\ue77f {{ .Full }} {{ end }}",
          "type": "dotnet"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "#CD4277",
          "style": "plain",
          "template": "# ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 4
}